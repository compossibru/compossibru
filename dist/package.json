{
    "name": "@compossibru/compossibru",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/compossibru/compossibru.git"
    },
    "authors": [
        {
            "name": "Leonard Hertel",
            "email": "leonard.hertel@gmail.com"
        },
        {
            "name": "Maik Schmidt",
            "email": "maik.schmidt.hl@gmail.com"
        }
    ],
    "bin": {
        "compossibru": "dist/bin/cli.js"
    },
    "scripts": {
        "eslint": "npx eslint bin/*.js src/*.js",
        "test": "npm run test:unit",
        "test:unit": "npx jest --env=node /test/unit",
        "prebuild": "rm -rf dist/ && mkdir dist && cp -r package.json package-lock.json README.md LICENSE src bin ./dist",
        "build": "npx babel dist --out-dir dist",
        "postbuild": "chmod +x dist/bin/cli.js",
        "prepublish": "npm run build"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "npx commitlint -e $HUSKY_GIT_PARAMS",
            "pre-push": "npm run eslint && npm run test:unit"
        }
    },
    "dependencies": {
        "camelcase": "5.0.0",
        "commander": "2.19.0",
        "cosmiconfig": "5.0.7",
        "ejs": "2.6.1",
        "fs-extra": "7.0.1",
        "next": "7.0.2",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "uuid": "3.3.2"
    },
    "devDependencies": {
        "@commitlint/cli": "7.2.1",
        "@commitlint/config-conventional": "7.1.2",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.3",
        "babel-eslint": "10.0.1",
        "babel-preset-es2015": "6.24.1",
        "eslint": "5.9.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-plugin-import": "2.14.0",
        "husky": "1.2.0",
        "jest": "23.6.0"
    }
}

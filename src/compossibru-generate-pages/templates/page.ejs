import React from 'react'
<% styles.forEach(function(style){ %>
import '<%= style %>';
<% }); %>

class Widget extends React.Component {
    componentDidMount() {
        <% widgetImports.forEach(function(widgetImport){ %>
        const <%= widgetImport.name %> = require('<%= widgetImport.path %>').default;
        <% }); %>
        <% widgetExecutions.forEach(function(widgetExecution){ %>
            <%= widgetExecution.name %>('<%= widgetExecution.id %>', <%- widgetExecution.context %>);
        <% }); %>
    }

    render() {
        return (
            <%- include(layoutPath, layoutVariables) -%>
        )
    }
}

export default Widget

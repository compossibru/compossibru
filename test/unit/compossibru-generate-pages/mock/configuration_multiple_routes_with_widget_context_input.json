{
    "Version": 1,
    "Styles": [
        "bootstrap/dist/css/bootstrap.min.css"
    ],
    "Routes": {
        "Home": {
            "Route": "/",
            "Layout": "layouts/3-column.ejs",
            "Containers": {
                "left": [
                    {
                        "compossibru-widget-jquery-example": {
                            "foo": "bar"
                        }
                    }
                ],
                "middle": [
                    "compossibru-widget-react-example"
                ],
                "right": [
                    "compossibru-widget-vue-example"
                ]
            }
        },
        "Foo": {
            "Route": "/foo",
            "Layout": "layouts/2-column.ejs",
            "Containers": {
                "left": [
                    "compossibru-widget-vue-example"
                ],
                "right": [
                    {
                        "compossibru-widget-jquery-example": {
                            "bar": "baz"
                        }
                    }
                ]
            }
        },
        "Bar": {
            "Route": "/bar",
            "Layout": "layouts/2-column.ejs",
            "Containers": {
                "left": [
                    {
                        "compossibru-widget-vue-example": {
                            "baz": [
                                "foo",
                                "bar"
                            ]
                        }
                    }
                ],
                "right": [
                    "compossibru-widget-react-example"
                ]
            }
        }
    }
}
